@CustomDialogexport struct LoadingDialog {  @State  message: string = ''  controller: CustomDialogController  build() {    Column({ space: 10 }) {      LoadingProgress().width(30).height(30).color('#fff')      if (this.message) {        Text(this.message).fontSize((14)).fontColor('#fff')      }    }    .justifyContent(FlexAlign.Center)    .alignItems(HorizontalAlign.Center)    .width(100)    .height(100)    .padding(10)    .backgroundBlurStyle(BlurStyle.BACKGROUND_ULTRA_THICK, {      colorMode: ThemeColorMode.DARK,      adaptiveColor: AdaptiveColor.AVERAGE,      scale: 0.1    })    .borderRadius(20)  }}