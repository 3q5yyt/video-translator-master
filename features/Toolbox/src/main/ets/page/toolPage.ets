import { toolBarUserShowPage } from "../components/toolPageComponent"//工具箱页面@Componentexport struct toolPage {  bottomRectHeight: string = AppStorage.get<number>('bottomRectHeight') + 'px';  topRectHeight: string = AppStorage.get<number>('topRectHeight') + 'px';  build() {    Column() {      Text('工具箱')        .fontWeight(800)        .fontSize(22)        .margin({ bottom: 10, top: 10 })      //工具列表      Scroll() {        Column() {          //视频剪辑成GIF和实况的工具          toolBarUserShowPage()        }        .alignItems(HorizontalAlign.Start)        .width('100%')      }      .scrollBar(BarState.Off)      .align(Alignment.Top)      .width('100%')      .layoutWeight(1)      .padding({ bottom: 80 })    }    .justifyContent(FlexAlign.Start)    .alignItems(HorizontalAlign.Start)    .padding({      top: this.topRectHeight,      bottom: this.bottomRectHeight,      left: 10,      right: 10    })    .width('100%')    .height('100%')  }}