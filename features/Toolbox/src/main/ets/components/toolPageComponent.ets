import { VideoToAnimationStyle } from 'tool';import { VideoToAnimationParam } from './VideoToAnimation';import { curves } from '@kit.ArkUI';/** * 视频剪辑成实况和GIF的工具 */@Componentexport struct toolBarUserShowPage {  @Consume('nimblePathStack') nimblePathStack: NavPathStack;  bottomRectHeight: string = AppStorage.get<number>('bottomRectHeight') + 'px';  topRectHeight: string = AppStorage.get<number>('topRectHeight') + 'px';  @State private isShowTip: boolean = false // 显示操作使用手册  build() {    Column() {      Stack({ alignContent: Alignment.TopStart }) {        //主要背景        Image($r('app.media.GIFBack'))          .width('100%')          .height('100%')          .objectFit(ImageFit.Cover)        //两个脚的图片        Row() {          Image($r('app.media.GIFLeftIcon'))            .height('20%')            .objectFit(1)            .alignSelf(ItemAlign.Start)          Blank()          Image($r('app.media.GIFLeftIcon01'))            .height('100%')            .objectFit(1)            .alignSelf(ItemAlign.Center)        }        .justifyContent(FlexAlign.Start)        .width('100%')        .height('100%')        .padding(10)        //操作手册        Column() {          Row() {            Text('操作使用手册')              .fontWeight(800)              .fontSize(30)              .fontColor(Color.White)              .zIndex(2)              .rotate({                angle: -4              })            Image($r('app.media.paintbrush'))              .width(20)              .objectFit(1)          }          .alignItems(VerticalAlign.Top)          Row() {            Text('转换器使用前须知')              .fontSize(12)              .fontWeight(600)          }          .rotate({            angle: -4          })          .zIndex(1)          .backgroundColor('#fad043')          .justifyContent(FlexAlign.Center)          .alignItems(VerticalAlign.Center)          .width(130)          .height(25)          .margin({ top: 1, left: 8 })        }        .justifyContent(FlexAlign.Center)        .alignItems(HorizontalAlign.Start)        .width('100%')        .height('100%')        .padding({ left: 40 })      }      .clip(true)      .width('100%')      .height(100)      .margin({ bottom: 15 })      .borderRadius(16)      .bindContentCover($$this.isShowTip, this.showTipImage(), {        transition: TransitionEffect.move(TransitionEdge.END)          .animation({ duration: 300, curve: Curve.EaseInOut }),      })      .onClick(() => {        this.isShowTip = true      })      .transition(TransitionEffect.move(TransitionEdge.TOP).animation({        curve: curves.springMotion(0.6, 0.8)      }))      Row({ space: 15 }) {        Stack() {          Image($r('app.media.VideoToGIFBack'))            .width('100%')            .height('100%')            .objectFit(ImageFit.Cover)          Column() {            Image($r('app.media.VideoToGIFText'))              .width('90%')              .objectFit(1)              .alignSelf(ItemAlign.Start)            Blank()            Image($r('app.media.VideoToGIFIcon'))              .width('90%')              .alignSelf(ItemAlign.End)              .objectFit(1)          }          .justifyContent(FlexAlign.Start)          .width('100%')          .height('100%')          .padding(10)        }        .clip(true)        .onClick(() => {          this.nimblePathStack.pushPathByName('VideoToAnimation', {            videoStyle: VideoToAnimationStyle.GIF          } as VideoToAnimationParam)        })        .aspectRatio(0.7)        .layoutWeight(1)        .borderRadius(24)        .transition(TransitionEffect.move(TransitionEdge.START).animation({          curve: curves.springMotion(0.6, 0.8)        }))        Stack() {          Image($r('app.media.VideoToLiveBack'))            .width('100%')            .height('100%')            .objectFit(ImageFit.Cover)          Column() {            Image($r('app.media.VideoToLiveText'))              .width('90%')              .objectFit(1)              .alignSelf(ItemAlign.Start)            Blank()            Image($r('app.media.VideoToLiveIcon'))              .width('90%')              .alignSelf(ItemAlign.End)              .objectFit(1)          }          .justifyContent(FlexAlign.Start)          .width('100%')          .height('100%')          .padding(10)        }        .onClick(() => {          this.nimblePathStack.pushPathByName('VideoToAnimation', {            videoStyle: VideoToAnimationStyle.LIVE          } as VideoToAnimationParam)        })        .clip(true)        .aspectRatio(0.7)        .layoutWeight(1)        .borderRadius(24)        .transition(TransitionEffect.move(TransitionEdge.END).animation({          curve: curves.springMotion(0.6, 0.8)        }))      }      .justifyContent(FlexAlign.Start)      .width('100%')    }    .width('100%')  }  //操作使用手册  @Builder  showTipImage() {    Column() {      //top栏      Row() {        Row() {          Image($r('app.media.chevron_left'))            .fillColor($r('app.color.textColor02'))            .width(40)            .height(40)            .margin({ right: 8 })            .padding(11.5)            .borderRadius(20)            .backgroundColor($r('app.color.backgroundColorForLeft'))            .onClick(() => {              this.isShowTip = false            })          Text('操作使用手册')            .fontSize(20)            .fontWeight(FontWeight.Bold)        }        .justifyContent(FlexAlign.Start)        .transition(TransitionEffect.translate({ x: 50, y: 0 })          .animation({ duration: 500, curve: Curve.LinearOutSlowIn }))      }      .padding({ left: 10, right: 10 })      .width('100%')      .margin({ bottom: 15 })      Scroll() {        //图片        Image($r('app.media.OperationManual'))          .width('100%')          .objectFit(1)      }      .scrollBar(BarState.Off)      .width('100%')      .layoutWeight(1)      .flexGrow(1)    }    .padding({ top: this.topRectHeight })    .backgroundColor($r('app.color.primaryColor'))    .width('100%')    .height('100%')  }}