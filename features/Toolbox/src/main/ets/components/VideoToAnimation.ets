import { VideoToAnimationStyle, VideoToLiveWallpaper } from "tool";import { window } from "@kit.ArkUI";import { BusinessError } from "@kit.BasicServicesKit";export interface VideoToAnimationParam {  videoStyle: VideoToAnimationStyle}@Componentexport struct VideoToAnimation {  @Consume('nimblePathStack') nimblePathStack: NavPathStack  private context: Context = this.getUIContext().getHostContext()!  @State private videoStyle: VideoToAnimationStyle = VideoToAnimationStyle.LIVE  //设置状态栏颜色(深色，浅色)  statusBarColor(model: boolean) {    if (!this.context) {      return    }    window.getLastWindow(this.context, async (err: BusinessError, data) => {      if (model) {        let SystemBarProperties: window.SystemBarProperties = {          statusBarContentColor: '#F1F3F5'        }        await data.setWindowSystemBarProperties(SystemBarProperties);      } else {        let SystemBarProperties: window.SystemBarProperties = {          statusBarContentColor: '#2F2F2F'        }        await data.setWindowSystemBarProperties(SystemBarProperties);      }    })  }  aboutToAppear() {    //改变状态栏颜色    this.statusBarColor(true)    //获取路由信息    const res: VideoToAnimationParam[] =      this.nimblePathStack.getParamByName('VideoToAnimation') as VideoToAnimationParam[]    this.videoStyle = res[0].videoStyle  }  aboutToDisappear() {    this.statusBarColor(false)  }  build() {    NavDestination() {      VideoToLiveWallpaper({ videoStyle: this.videoStyle })        .padding({ top: 10 })    }    .backgroundColor('#1c1c1c')    .hideTitleBar(true)    .hideToolBar(true)    .width('100%')    .height('100%')  }}